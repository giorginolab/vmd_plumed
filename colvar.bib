@article{Torrie_Valleau_1977,
  title =	 {Nonphysical sampling distributions in Monte Carlo
                  free-energy estimation: Umbrella sampling},
  volume =	 {23},
  DOI =		 {10.1016/0021-9991(77)90121-8},
  abstractNote = {The free energy difference between a model system
                  and some reference system can easily be written as
                  an ensemble average, but the conventional Monte
                  Carlo methods of obtaining such averages are
                  inadequate for the free-energy case. That is because
                  the Boltzmann-weighted sampling distribution
                  ordinarily used is extremely inefficient for the
                  purpose. This paper describes the use of arbitrary
                  sampling distributions chosen to facilitate such
                  estimates. The methods have been tested successfully
                  on the Lennard-Jones system over a wide range of
                  temperature and density, including the gas-liquid
                  coexistence region, and are found to be extremely
                  powerful and economical.},
  number =	 {2},
  journal =	 {Journal of Computational Physics},
  author =	 {Torrie, G. M. and Valleau, J. P.},
  year =	 {1977},
  pages =	 {187 -199}
}

